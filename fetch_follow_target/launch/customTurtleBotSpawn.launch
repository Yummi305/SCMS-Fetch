<launch>

<!--Spawn turtlebot3 from turtlebot repository-->
  <arg name="x_pos" default="0.0"/>
  <arg name="y_pos" default="-2.0"/>
  <arg name="z_pos" default="0.0"/>
  <arg name="yaw_pos" default ="-1.57"/>
  <arg name="sdf_robot_file" default=""/>

<!--include gazebo node to spawn model in gazebo-->
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" respawn="false" output="screen"
  args="-file $(arg sdf_robot_file) -sdf -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw_pos) "/>
 <!--Use turtlebot's teleop key to move the bot in gazebo-->
  <node pkg="fetch_follow_target" type="turtlebot3_teleop_key" name="turtlebot3_teleop_keyboard"  output="screen">
  </node>
</launch>
